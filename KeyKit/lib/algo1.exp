# This is a set of transformations for the expresso tool
A=A+A
A=scadjust(A,scale_newage())
# A=scafilt(A,scale_newage())
A=transpose(A,4)
A=transpose(A,-5)
# A=transpose(A,-7)
# A=A+transpose(A,12)
A=A+transpose(A,7)
A=A+transpose(A,4)+transpose(A,7)
# A=echo(A,3,24,0.5)
A=echo(A,3,12,0.5)
A=step(A,12)
A=preecho(A,5,6,0.7)
A=preecho(A,32,12,0.9)
A=stutterrand(A)
A=arpeggio(A)
A=flip(A)
A=chordmel(A)
